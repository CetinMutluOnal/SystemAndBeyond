<div class="search">
    <h1 class="search__heading">Search Results</h1>
</div>

<section class="posts">

    <ul class="post-ul">
        <% data.forEach(post => { %>
        <li> 
            <a href="/post/<%= post._id %>">
                <span> <%= post.title %> </span>
                <span class="post-list__date"><%= post.createdAt.toDateString()  %></span>
            </a>
        </li>
        <% }) %>
    </ul>

</section>